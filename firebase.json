{
  "hosting": {
    "target": "senseidb-agent",
    "public": "frontend",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "senseidb-agent",
          "region": "southamerica-east1"
        }
      }
    ]
  }
}